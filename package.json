{
  "name": "zipcodes-ph",
  "version": "0.0.0-semantically-released",
  "description": "Philippines zip code directory",
  "main": "source/index.js",
  "scripts": {
    "check-coverage": "istanbul check-coverage --statements 100 --branches 100 --functions 100 --lines 100",
    "commit": "git-cz",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post",
    "test": "mocha source/index.test.js -w",
    "test:single": "istanbul cover -x *.test.js _mocha -- source/index.test.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/arnellebalane/zipcodes-ph.git"
  },
  "keywords": [
    "philippines",
    "zipcodes"
  ],
  "author": "Arnelle Balane <arnellebalane@gmail.com> (https://arnellebalane.com/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/arnellebalane/zipcodes-ph/issues"
  },
  "homepage": "https://github.com/arnellebalane/zipcodes-ph#readme",
  "devDependencies": {
    "commitizen": "2.9.6",
    "cz-conventional-changelog": "2.1.0",
    "ghooks": "2.0.0",
    "istanbul": "0.4.5",
    "mocha": "4.0.1",
    "semantic-release": "^8.2.0"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    },
    "ghooks": {
      "pre-commit": "npm run test:single && npm run check-coverage"
    }
  }
}
